-- Scripts:

local function RHASDL_fake_script() -- CommandsBtn.LocalScript 
	local script = Instance.new('LocalScript', CommandsBtn)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.CommandsFrm.Visible = true
		script.Parent.Parent.ToolsFrm.Visible = false
		script.Parent.Parent.ScriptHubFrm.Visible = false
	end)
end
coroutine.wrap(RHASDL_fake_script)()
local function GTVFGT_fake_script() -- ToolsBtn.LocalScript 
	local script = Instance.new('LocalScript', ToolsBtn)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.CommandsFrm.Visible = false
		script.Parent.Parent.ToolsFrm.Visible = true
		script.Parent.Parent.ScriptHubFrm.Visible = false
	end)
end
coroutine.wrap(GTVFGT_fake_script)()
local function JITJNKU_fake_script() -- ScriptHubBtn.LocalScript 
	local script = Instance.new('LocalScript', ScriptHubBtn)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.CommandsFrm.Visible = false
		script.Parent.Parent.ToolsFrm.Visible = false
		script.Parent.Parent.ScriptHubFrm.Visible = true
	end)
end
coroutine.wrap(JITJNKU_fake_script)()
local function NQPM_fake_script() -- CloseBtn.LocalScript 
	local script = Instance.new('LocalScript', CloseBtn)

	script.Parent.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.PlayerGui.ScreenGui.Frame.Visible = false
	end)
end
coroutine.wrap(NQPM_fake_script)()
local function NCIHQG_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(NCIHQG_fake_script)()
local function RMNVFQ_fake_script() -- OpenBtn.LocalScript 
	local script = Instance.new('LocalScript', OpenBtn)

	script.Parent.MouseButton1Click:Connect(function()
		game.Players.LocalPlayer.PlayerGui.ScreenGui.Frame.Visible = true
	end)
end
coroutine.wrap(RMNVFQ_fake_script)()

-- FE Admin

 game.StarterGui:SetCore("SendNotification", {
    Title = "FE Admin | (ALPHA)"; 
    Text = "Injected"; 
    Icon = "http://www.roblox.com/asset/?id=50540854"; 
    Duration = 5;
    Callback = bindableFunction;
    Button1 = "";  
    Button2 = "";
    })


local prefix = ";"
local LPlayer = game.Players.LocalPlayer
LPlayer.Chatted:Connect(function(msg)
	msg = msg:lower()
	if string.sub(msg,1,3) == "" then
		msg = string.sub(msg,4)
	end
	if string.sub(msg,1,1) == prefix then
		local cmd
		local space = string.find(msg," ")
		if space then
			cmd = string.sub(msg,2,space-1)
		else
			cmd = string.sub(msg,2)
		end
		
		if cmd == "print" then
			local var = string.sub(msg,space+1)
			print(var)
		end
		if cmd == "re" then
			game.Players.LocalPlayer.Character:BreakJoints()
		end
        if cmd == "infjump" then
			loadstring(game:HttpGet("https://pastebin.com/raw/guJ5wajR"))()
		end
         if cmd == "speed(C)" then
			loadstring(game:HttpGet("https://raw.githubusercontent.com/angeloX041/Speedc/main/SuperSpeedC "))()
		end
        if cmd == "chatSpy" then
			loadstring(game:HttpGet("https://pastebin.com/raw/MwmrpsPK"))()
		end
        if cmd == "esp" then
			loadstring(game:HttpGet("https://pastebin.com/raw/5Uj8i3Ja"))()
		end
        
	    
		
	end
end)

function GetPlayer(String)
    local plr
    local strl = String:lower()
        for i, v in pairs(game:GetService("Players"):GetPlayers()) do
            if v.Name:lower():sub(1, #String) == String:lower() then
                plr = v
            end 
        end
        if String == "me" then
                plr = game.Players.LocalPlayer
            end
        if String == "" or String == " " then
           plr = nil
        end
    return plr
end
