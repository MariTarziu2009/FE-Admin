-- Scripts:

local function YOTY_fake_script() -- MainBtn.LocalScript 
	local script = Instance.new('LocalScript', MainBtn)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.MainFrm.Visible = true
		script.Parent.Parent.ToolsFrm.Visible = false
		script.Parent.Parent.ScriptHubFrm.Visible = false
		script.Parent.Parent.MiscFrm.Visible = false
	end)
end
coroutine.wrap(YOTY_fake_script)()
